<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="359.0" prefWidth="686.0" style="-fx-background-color: #03224C;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AnnoncesArtisanController">
   <children>
      <TableView fx:id="listAnnonce" prefHeight="332.0" prefWidth="696.0">
         <columns>
            <TableColumn fx:id="nomAnnonce" prefWidth="105.0" text="Nom Annonce" />
            <TableColumn fx:id="type" minWidth="0.0" prefWidth="94.0" text="Type Annonce" />
            <TableColumn fx:id="prix" prefWidth="42.0" text="\%" />
            <TableColumn fx:id="description" prefWidth="454.0" style="white-space: normal;" text="Description" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="Ajout" layoutX="21.0" layoutY="333.0" mnemonicParsing="false" onAction="#AjouterAnn" style="-fx-background-color: #03224C;" text="Ajouter Annonce" textFill="WHITE" />
      <Button fx:id="suppBtn" layoutX="149.0" layoutY="333.0" mnemonicParsing="false" onAction="#delete" style="-fx-background-color: #03224C;" text="Supprimer Annonce" textFill="WHITE" />
      <Button fx:id="StatBtn" layoutX="538.0" layoutY="333.0" mnemonicParsing="false" onAction="#StatType" style="-fx-background-color: #03224C;" text="StatistiqueAnnonces" textFill="WHITE" />
      <Button fx:id="ModifBtn" layoutX="348.0" layoutY="333.0" mnemonicParsing="false" onAction="#Modif" style="-fx-background-color: #03224C;" text="Modifier Annonce" textFill="WHITE" />
   </children>
</AnchorPane>
